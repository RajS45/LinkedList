


class Solution {
    public boolean isPalindrome(ListNode head) {
        Stack<Integer> st = new Stack<>();
        ListNode current = head;

        while (current != null) {
            st.push(current.data);
            current = current.next;
        }

        current = head;
        while (current != null) {
            if (current.data != st.pop()) {
                return false;
            }
            current = current.next;
        }

        return true;
    }
}
